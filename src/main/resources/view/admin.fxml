<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.AnchorPane?>
<?import javafx.scene.text.Font?>

<AnchorPane maxHeight="730.0" maxWidth="900.0" prefHeight="730.0" prefWidth="900.0" style="-fx-background-color: #2F4F4F;" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="com.verbovskiy.client.controller.AdminInterfaceController">
   <children>
      <Button fx:id="buttonAccount" layoutX="250.0" layoutY="325.0" mnemonicParsing="false" onAction="#openAccountsManagementInterface" prefHeight="40.0" prefWidth="400.0" text="Управление учетными записями " textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <Button fx:id="buttonExitAdmin" layoutX="250.0" layoutY="574.0" mnemonicParsing="false" onAction="#exitAdmin" prefHeight="40.0" prefWidth="400.0" text="Вернуться назад" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="15.0" />
         </font></Button>
      <AnchorPane prefHeight="170.0" prefWidth="900.0" style="-fx-background-color: #F0FFF0;" />
      <Button fx:id="buttonAddCar" layoutX="250.0" layoutY="247.0" mnemonicParsing="false" onAction="#addCar" prefHeight="40.0" prefWidth="400.0" text="Добавить машину" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonShowCars" layoutX="250.0" layoutY="400.0" mnemonicParsing="false" onAction="#showCars" prefHeight="40.0" prefWidth="400.0" text="Просмотр машин" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
      <Button fx:id="buttonShowOrders" layoutX="250.0" layoutY="478.0" mnemonicParsing="false" onAction="#showOrders" prefHeight="40.0" prefWidth="400.0" text="Просмотр заказов" textFill="DARKSLATEGRAY">
         <font>
            <Font name="System Bold" size="15.0" />
         </font>
      </Button>
   </children>
</AnchorPane>
