      <TextField fx:id="textFieldSurname" layoutX="245.0" layoutY="76.0" prefHeight="46.0" prefWidth="359.0" promptText="фамилия" style="-fx-background-color: #F0FFF0;">
         <font>
            <Font name="System Bold Italic" size="16.0" />
         </font>
      </TextField>
   </children>
</AnchorPane>
